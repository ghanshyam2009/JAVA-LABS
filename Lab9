		JTextField t1 = new JTextField();
		t1.setBounds(100, 370, 80, 30);
		f.add(t1);
		
		JButton l5 = new JButton("Send");
		l5.setBounds(200, 370, 80, 30);
		f.add(l5);
		
		JButton l6 = new JButton("Reset");
		l6.setBounds(300, 370, 80, 30);
		f.add(l6);
		
		f.setLayout(null);
		f.getContentPane();
