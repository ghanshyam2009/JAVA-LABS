//		JPanel p1 = new JPanel();
//		p1.setBounds(0, 0, 800, 30);
//		f.add(p1);
		
		JButton l1 = new JButton("File");
		l1.setBounds(0, 0, 80, 30);
		f.add(l1);
		
		JButton l2 = new JButton("Help");
		l2.setBounds(80, 0, 80, 30);
		f.add(l2);
		
		JButton l3 = new JButton("Open");
		l3.setBounds(0, 30, 80, 30);
		f.add(l3);
		
		JButton l4 = new JButton("Save as");
		l4.setBounds(0, 60, 80, 30);
		f.add(l4);
		
//		JPanel p2 = new JPanel();
//		p2.setBounds(0, 300, 800, 30);
//		f.add(p2);
		
		JLabel lb1 = new JLabel("Enter Text ");
